<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">添加课程</view></cu-custom><view class="cu-form-group margin-top"><view class="title">课程名称</view><input placeholder="请输入课程名称" type="text" maxlength="30" focus="false" data-event-opts="{{[['input',[['setLessonName',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">课程类别</view><picker mode="multiSelector" value="{{multiIndex}}" range="{{multiArray}}" data-event-opts="{{[['change',[['MultiChange',['$event']]]],['columnchange',[['MultiColumnChange',['$event']]]]]}}" bindchange="__e" bindcolumnchange="__e"><view class="picker" style="text-align:left;">{{''+multiArray[0][multiIndex[0]]+"，"+multiArray[1][multiIndex[1]]+''}}</view></picker></view><view class="cu-form-group"><view class="title">课时数</view><input placeholder="请输入课时数" type="number" maxlength="9" focus="false" data-event-opts="{{[['input',[['setLessonTimes',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">课程价格</view><input placeholder="请输入课程价格" type="digit" maxlength="10" focus="false" data-event-opts="{{[['input',[['setLessonPrice',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-form-group align-start"><view class="title">课程介绍</view><textarea maxlength="140" disabled="{{modalName!=null}}" placeholder="请输入课程介绍" data-event-opts="{{[['input',[['setLessonIntro',['$event']]]]]}}" bindinput="__e"></textarea></view><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['createCommit',['$event']]]]]}}" class="cu-btn bg-mauve margin-tb-sm lg" bindtap="__e">提交</button></view></view>