<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">享淘课</view></cu-custom><view class="box"><view class="cu-bar tabbar bg-white foot shadow"><block wx:for="{{tabbar}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['switchTo',['$event','$0'],[[['tabbar','',index,'url']]]]]]]}}" class="action" bindtap="__e"><view class="{{['cuIcon-cu-image',item.choosed?'text-green':'text-grey']}}"><text class="{{['lg','cuIcon-'+item.icon]}}"></text></view><view class="{{[item.choosed?'text-green':'text-grey']}}">{{item.title}}</view></view></block></view></view><view class="{{['cu-list grid','col-'+gridCol,gridBorder?'':'no-border']}}"><block wx:for="{{cuIconList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index<gridCol*2}}"><view data-event-opts="{{[['tap',[['jumpTo',['$event','$0'],[[['cuIconList','',index,'url']]]]]]]}}" class="cu-item" bindtap="__e"><view class="{{['cuIcon-'+item.cuIcon,'text-'+item.color]}}"><block wx:if="{{item.badge!=0}}"><view class="cu-tag badge"><block wx:if="{{item.badge!=1}}"><block>{{item.badge>99?'99+':item.badge}}</block></block></view></block></view><text class="{{['text-'+item.color]}}">{{item.name}}</text></view></block></block></view><view class="grid col-2 padding-sm"><block wx:for="{{catalog}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="padding-sm"><view data-event-opts="{{[['tap',[['jumpTo',['$event','$0'],[[['catalog','',index,'url']]]]]]]}}" class="bg-gradual-red padding radius text-center shadow-blur" bindtap="__e"><view class="text-lg">{{item.title}}</view></view></view></block></view><view class="padding-xl"><button data-event-opts="{{[['tap',[['toIndex',['$event']]]]]}}" class="cu-btn block line-orange lg" bindtap="__e"><text class="cuIcon-explore"></text>example</button></view></view>