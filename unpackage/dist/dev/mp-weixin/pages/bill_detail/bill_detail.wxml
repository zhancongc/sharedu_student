<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">订单详情</view></cu-custom><view class="{{['cu-card article',isCard?'no-card':'']}}"><block wx:for="{{bills}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item shadow"><view class="title"><view class="text-cut">{{item.title}}</view></view><view class="content"><image src="{{item.image}}" mode="aspectFill"></image><view class="desc"><view class="text-title"><view class="cu-tag bg-orange light sm round">{{item.city}}</view></view><view class="text-content"><view class="avatar-text"><text class="text-price" style="color:red;">{{''+item.price}}</text></view><view class="avatar-text">{{item.times+"课时"}}</view></view></view></view></view></block></view></view>