<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">店铺入驻</view></cu-custom><view class="cu-form-group margin-top"><view class="title">邀请码</view><input placeholder="请输入代理商提供的邀请码" name="input" type="text" maxlength="10" focus="false" data-event-opts="{{[['input',[['setInvitationCode',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">商户名称</view><input placeholder="请输入商户名称" name="input" type="text" maxlength="30" focus="true" data-event-opts="{{[['input',[['setStoreName',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">详细地址</view><input placeholder="请输入商户地址" name="input" type="text" maxlength="30" focus="true" data-event-opts="{{[['input',[['__set_model',['','storeAddress','$event',[]]],['setStoreAddress',['$event']]]]]}}" value="{{storeAddress}}" bindinput="__e"/><text data-event-opts="{{[['tap',[['getLocation',['$event']]]]]}}" class="cuIcon-locationfill text-orange" bindtap="__e"></text></view><view class="cu-form-group"><view class="title">负责人</view><input placeholder="请输入商户主要负责人" name="input" type="text" maxlength="12" focus="true" data-event-opts="{{[['input',[['setManager',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">联系手机</view><input placeholder="请输入负责人手机号码" name="input" type="number" maxlength="11" focus="true" data-event-opts="{{[['input',[['setManagerPhone',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-bar bg-white margin-top"><view class="action">上传营业执照图片</view><view class="action">{{''+licenseImageList.length+'/4'}}</view></view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block wx:for="{{licenseImageList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="bg-img" data-url="{{licenseImageList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{licenseImageList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{licenseImageList.length<4}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view><view class="cu-bar bg-white margin-top"><view class="action">上传商户经营场所图片</view><view class="action">{{''+storeImageList.length+'/4'}}</view></view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block wx:for="{{storeImageList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="bg-img" data-url="{{storeImageList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{storeImageList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{storeImageList.length<4}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['createCommit',['$event']]]]]}}" class="cu-btn bg-mauve margin-tb-sm lg" bindtap="__e">提交</button></view></view>